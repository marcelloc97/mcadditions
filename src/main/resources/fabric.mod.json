{
	"schemaVersion": 1,
	"id": "mcadditions",
	"version": "${version}",
	"name": "MC Additions",
	"description": "Adds some new content and mechanics for Minecraft Java. Some contents are 'copied' from Kaupenjoe's tutorial for 1.20.1-fabric. Hope you enjoy it!",
	"authors": [
		"mcdev - marcelloc97"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mcadditions/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.mcdev.mcadditions.MCAdditions"
		],
		"client": [
			"net.mcdev.mcadditions.MCAdditionsClient"
		],
		"fabric-datagen": [
			"net.mcdev.mcadditions.MCAdditionsDataGenerator"
		]
	},
	"mixins": [
		"mcadditions.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}